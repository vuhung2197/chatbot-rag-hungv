{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "#services/*": [
                "./services/*"
            ],
            "#modules/*": [
                "./src/modules/*"
            ],
            "#shared/*": [
                "./src/shared/*"
            ],
            "#utils/*": [
                "./utils/*"
            ],
            "#db": [
                "./db.js"
            ],
            "#bootstrap/*": [
                "./bootstrap/*"
            ],
            "#helpers/*": [
                "./helpers/*"
            ],
            "#middlewares/*": [
                "./middlewares/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}